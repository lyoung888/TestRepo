{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ADFlyTutorialDataFactory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStore_Source')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "AzureDataLakeStore",
				"typeProperties": {
					"dataLakeStoreUri": "https://adflydatalake.azuredatalakestore.net/webhdfs/v1",
					"tenant": "487cc98f-8695-4e3f-87b5-87e15cc0bc31",
					"subscriptionId": "22631579-e944-4e57-b147-d1ddba73ecd3",
					"resourceGroupName": "adfLYrg"
				}
			}
		}
	]
}